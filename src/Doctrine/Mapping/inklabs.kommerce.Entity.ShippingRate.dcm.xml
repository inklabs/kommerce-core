<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <embeddable name="inklabs\kommerce\Entity\ShippingRate">

        <field name="code" type="string" length="255" nullable="true"/>
        <field name="name" type="string" length="255" nullable="true"/>
        <field name="cost" type="string" length="255" nullable="true"/>

        <field name="deliveryTs" type="integer" nullable="true">
            <options>
                <option name="unsigned">true</option>
            </options>
        </field>

        <field name="transitTime" type="string" length="255" nullable="true"/>

        <field name="weightInPounds" type="integer" nullable="true">
            <options>
                <option name="unsigned">true</option>
            </options>
        </field>

        <field name="shipMethod" type="string" length="255" nullable="true"/>
        <field name="zip5" type="string" length="5" nullable="true"/>
        <field name="state" type="string" length="2" nullable="true"/>
        <field name="isResidential" type="boolean" nullable="true"/>

    </embeddable>

</doctrine-mapping>
